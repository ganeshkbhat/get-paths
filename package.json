  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/eslint": "^8.56.2",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.16",
    "tape": "^4.9.0",
    "chai": "^4.4.1",
    "live-server": "^1.2.2",
    "mocha": "^10.2.0",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.4",
    "sinon": "^17.0.1",
    "typescript": "^5.3.3",
    "uglify-js": "^3.17.4",
    "unimported": "^1.31.1"
  }
}
